{
    "version": 2,
    "builds": [
        {
            "src": "index.html",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "dist/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/index.html"
        }
    ]
}