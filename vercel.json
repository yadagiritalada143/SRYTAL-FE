{
    "version": 2,
    "builds": [
        {
            "src": "dist/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/index.html"
        },
        {
            "src": "/favicon.ico",
            "headers": {
                "Cache-Control": "public, max-age=3600"
            },
            "continue": true
        },
        {
            "src": "/assets/(.*)",
            "headers": {
                "Cache-Control": "public, max-age=31556952, immutable"
            },
            "continue": true
        }
    ]
}